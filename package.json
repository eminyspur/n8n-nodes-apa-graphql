{
  "name": "n8n-nodes-apa-graphql",
  "version": "0.1.0",
  "description": "n8n node for APA GraphQL API",
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Eminy Spur",
    "email": "eminyspur@gmail.com"
  },
  "keywords": ["n8n-community-node-package"],
  "repository": {
    "type": "git",
    "url": "https://github.com/eminyspur/n8n-nodes-apa-graphql.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json",
    "lintfix": "eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ApaApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Apa/Apa.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/parser": "~5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
